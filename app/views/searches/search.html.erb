<div class="container">
  <div class="row">
    <div class="col">
        <div class="text-center">
          <%= render 'search' %>
        </div>
        <h2>Result index</h2>
        <table class ="table table-hover table-inverse">
        <!--検索対象モデルがUserの時-->
        <% if @range == "User" %>
          <tbody>
            <tr>
              <td>Image</td>
              <td>Name</td>
            </tr>
          <% @users.each do |user| %>
            <tr>
              <td><%= image_tag user.get_profile_image, size: "100x100" %></td>
              <td><%= user.name %></td>
            </tr>
          <% end %>
          </tbody>
        <% else %>
          <!--検索対象モデルがUserでない時（＝検索対象モデルがBookの時）-->
          <tbody>
            <tr>
              <td colspan></td>
              <td>Title</td>
              <td>Opinion</td>
            </tr>
          <% @books.each do |book| %>
            <tr>
              <td><%= image_tag book.user.get_profile_image, size: "100x100" %></td>
              <td><%= link_to book.title, book_path(book.id) %></td>
              <td><%= book.body %></td>
            </tr>
          <% end %>
          </tbody>
        <% end %>
      </table>
      </div>
    </div>
  </div>